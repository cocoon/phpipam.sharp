<Window x:Class="HTTP_POST.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="421" Width="525">
    <StackPanel>
        <ComboBox x:Name="ComboBox1" Margin="50,10,50,0"/>
        <TextBox x:Name="TxtBoxInput" Height="23" TextWrapping="Wrap" Text="1" Margin="50,10,50,0" Padding="10,0,0,0"/>
        <Button x:Name="Btn1" Content="Button" Click="Btn1_Click" Width="80" Margin="0,10,0,0"/>
        <TextBox x:Name="TxtBox1" TextWrapping="Wrap" VerticalAlignment="Bottom" MinHeight="150" Margin="0,10,0,0"/>
        <TextBox x:Name="TxtBox2" TextWrapping="Wrap" MinHeight="150"/>
        <DataGrid x:Name="DataGrid1" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" MinHeight="150"/>
    </StackPanel>
</Window>
